
<p id="notice"><%= notice %></p>


<div class="container">

  <%= render('posts/search') %>



  <h1 class ="text-center">Posts</h1>

  <table>
    <thead>
      <tr>
        <th>Title</th>
        <th>Content</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody id="data">
      <%= render('posts/data') %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Post', new_post_path, remote: true, class: "btn btn-primary" %>

  <div id="createData" class = "my-5"></div>

  <div id="showData"></div>
